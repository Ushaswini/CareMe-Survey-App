@section Scripts {
    @Scripts.Render("~/bundles/MessageMange")
    @Scripts.Render("~/bundles/auth")
}

<div class="col-sm-12" style="float:left;margin:20px">
    <h3 style="margin-bottom:10px;margin-top:10px">Messages sent</h3>
    <table class="table table-hover table-bordered table-striped" id="surveysTable">
        <thead>
            <tr>
                <th>Question</th>
                <th>Frequency</th>
                <th>Response</th>
                <th>Response Time</th>
            </tr>
        </thead>

    </table>
   
</div>