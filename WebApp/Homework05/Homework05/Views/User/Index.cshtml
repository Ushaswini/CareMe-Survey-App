@section Scripts {
    @Scripts.Render("~/bundles/MessageMange")
    @Scripts.Render("~/bundles/auth")
}

<div class="col-sm-12" style="float:left">
    <h3 id="titleGroup" style="margin-bottom:10px;margin:10px">List of study groups</h3>
    <div class="col-sm-12" style="float:left;margin:10px">

        <table class="table table-hover table-bordered table-striped" id="groupsTable">
            <thead>
                <tr>
                    <th>Study-Group Name</th>
                    <th>CreatedTime</th>

                </tr>
            </thead>

        </table>
    </div>

</div>
<div class="col-sm-12" style="float:left">
    <h3 id ="title"style="margin-bottom:10px;margin:10px">List of users enrolled</h3>
    <div class="col-sm-12" style="float:left;margin:10px">

        <table class="table table-hover table-bordered table-striped" id="usersTable">
            <thead>
                <tr>
                    <th>User Name</th>
                    <th>Study-Group Name</th>

                </tr>
            </thead>

        </table>
    </div>

</div>

